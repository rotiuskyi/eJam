{
  "compilerOptions": {
    "moduleResolution": "node",
    "paths": {
      "shared": ["shared/"]
    },
    "skipLibCheck": true,
    "downlevelIteration": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "inlineSourceMap": true
  }
}
